    "    SimpleLinearRegression\n",
    "    ----------------------\n",
    "\n",
    "    y = mx + b\n",
    "    y -> dependent variable\n",
    "    x -> independent variable\n",
    "    b -> y intercept : constant\n",
    "    m -> slope coefficient\n",
